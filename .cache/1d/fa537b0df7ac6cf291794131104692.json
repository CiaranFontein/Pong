; j--) {\n      balls[j].radius = (j / this.length) * 10;\n      if (balls[j].radius < 0) {\n        balls[j].radius = 0;\n        balls[j].opacity = 0.01;\n      }\n    }\n  }\n}\n\nclass TrailBall {\n  constructor(x, y, radius, opacity) {\n    this.x = x;\n    this.y = y;\n    this.radius = radius;\n    this.opacity = opacity;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"60cef513fc0f4deea7ad15e7946a888c","cacheData":{"env":{}}}